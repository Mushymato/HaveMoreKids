{
  "$schema": "https://smapi.io/schemas/content-patcher.json",
  "Changes": [
    // 3 babies
    {
      "Action": "Include",
      "FromFile": "data/child.json",
      "LocalTokens": {
        "L@Hair": "{{L@Spouse}}Boy",
      }
    },
    {
      "Action": "Include",
      "FromFile": "data/child.json",
      "LocalTokens": {
        "L@Hair": "{{L@Spouse}}Girl",
      }
    },
    {
      "Action": "Include",
      "FromFile": "data/child.json",
      "LocalTokens": {
        "L@Hair": "{{L@Spouse}}Neutral",
      },
      "When": {
        "L@Spouse |contains=sam": false,
        "L@Spouse |contains=classic": false,
      }
    },
  ]
}

{
  "$schema": "https://smapi.io/schemas/content-patcher.json",
  "Changes": [
    // The toddler can have dialogue and gift tastes, but no schedules
    {
      // load blank.json
      "Action": "Load",
      "Target": "Characters/Dialogue/{{ModId}}_{{L@Kid}}",
      "FromFile": "data/blank.json"
    },
    {
      "Action": "EditData",
      "Target": "Characters/Dialogue/{{ModId}}_{{L@Kid}}",
      "Entries": {
        // this is an ordinary vanilla dialogue asset, and accepts all the same keys
        "Mon": "{{i18n:{{L@Kid}}.Toddler.Dialog.Mon}} {{DayOfWeek}}",
        "Tue": "{{i18n:{{L@Kid}}.Toddler.Dialog.Tue}} {{DayOfWeek}}",
        "Wed": "{{i18n:{{L@Kid}}.Toddler.Dialog.Wed}} {{DayOfWeek}}",
        "Thu": "{{i18n:{{L@Kid}}.Toddler.Dialog.Thu}} {{DayOfWeek}}",
        "Fri": "{{i18n:{{L@Kid}}.Toddler.Dialog.Fri}} {{DayOfWeek}}",
        "Sat": "{{i18n:{{L@Kid}}.Toddler.Dialog.Sat}} {{DayOfWeek}}",
        "Sun": "{{i18n:{{L@Kid}}.Toddler.Dialog.Sun}} {{DayOfWeek}}",
        // you can use FarmHouse keys for when kid is inside
        "FarmHouse": "I'm sleepy."
      }
    },
    {
      "Action": "EditData",
      "Target": "Data/NPCGiftTastes",
      "Entries": {
        // You can give the child some gift tastes, they also follow normal format
        "{{ModId}}_{{L@Kid}}": "{{i18n:{{L@Kid}}.Toddler.Gift.Loved}}/221 398 612 721 903 FrogEgg/{{i18n:{{L@Kid}}.Toddler.Gift.Liked}}/18 60 64 70 88 232 233 234 222 223 340 344 402 418 -6/{{i18n:{{L@Kid}}.Toddler.Gift.Dislike}}/-5 -75 -79 -81 80/{{i18n:{{L@Kid}}.Toddler.Gift.Hated}}/253 395 459 -26 16 330 873/{{i18n:{{L@Kid}}.Toddler.Gift.Neutral}}//",
      }
    }
  ]
}
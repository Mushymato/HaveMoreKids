{
  "$schema": "https://smapi.io/schemas/content-patcher.json",
  "Changes": [
    // This is data exclusive to Cookie
    // Since this is being included through kid.json, {{L@Kid}}='cookie'
    {
      "Action": "EditData",
      "Target": "mushymato.HaveMoreKids/ChildData",
      "Fields": {
        "{{ModId}}_{{L@Kid}}": {
          "DisplayName": "{{i18n:ARVCookie.DisplayName}}",
          "Gender": "Male",
          // They need a Home entries (to not spawn in middle of Town), just like normal NPCs
          "Home": [
            {
              "Id": "Default",
              "Location": "BusStop",
              "Tile": {
                "X": 22,
                "Y": 14
              },
              "Direction": "down"
            }
          ],
        }
      }
    },
    // Spirit's Eve appearance
    {
      "Action": "Load",
      "Target": "Characters/{{ModId}}_{{L@Kid}}/{{L@Kid}}_flynn",
      "FromFile": "assets/Characters/Pale/{{L@Kid}}/{{TargetWithoutPath}}.png"
    },
    {
      "Action": "Load",
      "Target": "Portraits/{{ModId}}_{{L@Kid}}/{{L@Kid}}_flynn",
      "FromFile": "assets/Portraits/Pale/{{L@Kid}}/{{TargetWithoutPath}}.png"
    },

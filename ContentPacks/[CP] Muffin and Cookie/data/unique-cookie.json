          "Id": "{{L@Kid}}_SpiritsEve_Flynn",
          "Portrait": "Portraits/{{ModId}}_{{L@Kid}}/{{L@Kid}}_flynn",
          "Sprite": "Characters/{{ModId}}_{{L@Kid}}/{{L@Kid}}_flynn",
          "Condition": "IS_EVENT festival_fall26",
          "Precedence": -1000,
        },
      }
    },

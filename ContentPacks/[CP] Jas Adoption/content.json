{
  "$schema": "https://smapi.io/schemas/content-patcher.json",
  "Format": "2.8.0",
  "Changes": [
    {
      "Action": "EditData",
      "Target": "mushymato.HaveMoreKids/Kids",
      "Entries": {
        // debug action mushymato.HaveMoreKids_SetNewChildEvent 0 Jas Player
        "Jas": {
          "BirthOrAdoptMessage": "Jas has moved in to the farm!",
          "AdoptedFromNPC": "Jas",
          // Add Jas to the adoption registry in the hospital, this is a GSQ
          "CanAdoptFromAdoptionRegistry": "TRUE"
        }
      },
    },
    {
      "Action": "EditData",
      "Target": "Data/Characters",
      "TargetField": [
        "Jas",
        "CustomFields"
      ],
      "Entries": {
        // control 
        "mushymato.HaveMoreKids/IsNPCToday": "DAY_OF_WEEK Tuesday",
        // optional: use alternate dialogue sheet name for the child version of Jas
        "mushymato.HaveMoreKids/KidDialogueSheet": "{{ModId}}_KidJasDialogue"
      }
    },
    // 
    {
      "Action": "Load",
      "Target": "Characters/Dialogue/{{ModId}}_KidJasDialogue",
      "FromFile": "blank.json"
    },
    {
      "Action": "EditData",
      "Target": "Characters/Dialogue/{{ModId}}_KidJasDialogue",
      "Entries": {
        "Mon": "Mon",
        "Tue": "Tue",
        "Wed": "Wed",
        "Thu": "Thu",
        "Fri": "Fri",
        "Sat": "Sat",
        "Sun": "Sun",
      }
    }
